{
  "name": "redbubble-code-test",
  "version": "1.0.0",
  "description": "The Code test for RedBubble job application",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/yuchenQ/redbubble-code-test.git"
  },
  "scripts": {
    "build": "NODE_ENV=production webpack --config webpack/webpack.config.prod.js",
    "start:dev": "NODE_ENV=development webpack-dev-server --config webpack/webpack.config.dev.js",
    "lint": "concurrently npm:lint:*",
    "lint:js": "eslint './src/**/*.{jsx,js}' --quiet --fix",
    "lint:styles": "stylelint './src/**/*.jsx'",
    "test": "concurrently \"npm:test:unit\" \"npm:test:snap\"",
    "test:unit": "jest src --config ./jest.config.json --coverage --coverageReporters=text --passWithNoTests",
    "test:snap": "jest storyshots --config ./jest.config.json",
    "test:snap:update": "npm run test:snap -- -u",
    "storybook": "start-storybook -p 6006"
  },
  "license": "MIT",
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -e $HUSKY_GIT_PARAMS",
      "pre-commit": "npm test && lint-staged",
      "pre-push": "npm test"
    }
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "pretty-quick",
      "eslint --fix",
      "git add"
    ],
    "*.{json,css,md}": [
      "pretty-quick",
      "git add"
    ]
  },
  "dependencies": {
    "@apollo/client": "^3.0.0-beta.44",
    "@babel/runtime-corejs3": "^7.9.6",
    "concurrently": "^5.2.0",
    "graphql": "^15.0.0",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "styled-components": "^5.1.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.6",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-react": "^7.9.4",
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@storybook/addon-backgrounds": "^5.3.18",
    "@storybook/addon-knobs": "^5.3.18",
    "@storybook/addon-notes": "^5.3.18",
    "@storybook/addon-storyshots": "^5.3.18",
    "@storybook/addon-viewport": "^5.3.18",
    "@storybook/addons": "^5.3.18",
    "@storybook/react": "^5.3.18",
    "@testing-library/jest-dom": "^5.5.0",
    "@testing-library/react": "^10.0.4",
    "babel-jest": "^25.5.1",
    "babel-loader": "^8.1.0",
    "babel-plugin-styled-components": "^1.10.7",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^3.5.3",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jest": "^23.9.0",
    "eslint-plugin-jest-dom": "^2.1.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^4.0.0",
    "html-loader": "^1.1.0",
    "html-webpack-plugin": "^4.3.0",
    "husky": "^4.2.5",
    "jest": "^25.5.4",
    "jest-styled-components": "^7.0.2",
    "jest-transform-stub": "^2.0.0",
    "jest-watch-typeahead": "^0.5.0",
    "lint-staged": "^10.2.2",
    "markdown-loader": "^5.1.0",
    "mini-css-extract-plugin": "^0.9.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "prettier": "^2.0.5",
    "pretty-quick": "^2.0.1",
    "react-test-renderer": "^16.13.1",
    "regenerator-runtime": "^0.13.5",
    "sinon": "^9.0.2",
    "stylelint": "^13.3.3",
    "stylelint-config-recommended": "^3.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.10.0",
    "terser-webpack-plugin": "^3.0.0",
    "url-loader": "^4.1.0",
    "webpack": "^4.43.0",
    "webpack-bundle-analyzer": "^3.7.0",
    "webpack-cli": "^3.3.11",
    "webpack-dashboard": "^3.2.0",
    "webpack-dev-server": "^3.10.3",
    "webpack-merge": "^4.2.2"
  }
}
